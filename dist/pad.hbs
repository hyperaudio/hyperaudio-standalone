<!doctype html>
<!-- **> init ** -->
<html lang="en" class="english">
<head>
<meta charset="utf-8">
<meta http-equiv="content-language" content="en">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width,height=device-height,user-scalable=no,initial-scale=1.0,maximum-scale=1.0,minimum-scale=1.0">
<meta http-equiv="cleartype" content="on">
<meta name="mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="translucent-black">
<meta name="apple-mobile-web-app-title" content="My App">

<title>Hyperaudio Pad</title>

<link rel="icon" type="image/x-icon" href="assets/images/favicon.ico">
<link rel="stylesheet" href="assets/fonts/hyperaudio-player/style.css">
<link rel="stylesheet" href="assets/styles/loader.css">

<script src="assets/scripts/vendorq1.js"></script>
</head>
<!-- **> init ** -->
<body class="tpl--pad tpl--compact">
<!-- **> aside-left** -->
<aside id="page-side--left" class="page-side page-side--left moved" data-side-push="right" data-side-state="inactive">
  <div class="page-side__body">
    <div id="page-menu" class="menu" data-active-pane="root">
      <div id="menu__pane--root" class="menu__pane">
        <div class="menu__body">
          <div class="headline" style="background-image: url('assets/images/covers/homeslides/slide-home.jpg')">
            <h2 class="headline__title"><a href="{{path}}">{{L.aside-left__title}}</a></h2>
            <p class="headline__text">{{L.aside-left__subtitle}}</p>
          </div>
          <form action="{{path}}search/">
            <div class="search">
              <input class="text-input menu__search" name="search" type="search" placeholder="{{L.search__placeholder}}" value=""/>
              <i class="icon-search"></i>
            </div>
          </form>
          <ol class="menu__list">
            <li class="menu__item">
              <a class="menu__link toggleMenuPane" data-target-menu-pane-id="documentaries">{{L.aside-left__films}} <span class="menu__link-icon"><i class="icon-point-right"></i></span></a>
            </li>
          </ol>
          <div class="menu__push"></div>
        </div>
        <div class="menu__foot">
          <span class="brandmark">
            <i class="icon-aj"></i>
          </span>
          <span class="footnotes">
            {{L.aside-left__footAJ}}<br/>
          </span>
        </div>
      </div>
      <div id="menu__pane--documentaries" class="menu__pane moved--right">
        <div class="headline" style="background-image: url('assets/images/covers/homeslides/slide-films.jpg')">
          <h2 class="headline__title">{{L.aside-left__films}}</h2>
          <p class="headline__text">{{L.aside-left__filmsintro}}</p>
        </div>
        <ol class="menu__list">
        <!-- **#each nav.films** -->
          <li class="menu__item"><a class="menu__link" href="XXX">XXX</a></li>
        <!-- **/each** -->
        </ol>
      </div>
      <div id="menu__pane--themes" class="menu__pane moved--right">
        <div class="headline" style="background-image: url('assets/images/covers/homeslides/slide-definitions.jpg')">
          <h2 class="headline__title">{{L.aside-left__glossary}}</h2>
          <p class="headline__text">{{L.aside-left__glossaryintro}}</p>
        </div>
        <ol class="menu__list">
        <!-- **#each nav.glossary** -->
          <li class="menu__item"><a class="menu__link" href="YYY">YYY</a></li>
        <!-- **/each** -->
        </ol>
      </div>
      <a id="menu-reset" class="menu-reset toggleMenuPane moved" data-target-menu-pane-id="root" aria-hidden="false">
        <span class="menu__link-icon">
          <i class="icon-point-left"></i>
        </span>
      </a>
    </div>
  </div>
</aside>
<!-- **> aside-left** -->
<header class="page-head" id="page-head" data-head-state="altered" data-head-state-persist="true">
<!-- **> header ** -->
<!-- **> header ** -->
</header>
<div id="page-body" class="page-body">
  <div class="HAP HAP--pad">
      <aside id="sidemenu" class="HAP-sidebar">
        <div class="search">
          <input class="text-input menu__search" name="search" id="HAP-search" type="search" placeholder="{{L.search__placeholder}}" value=""/>
          <i class="icon-search"></i>
        </div>
        <ul id="panel-media" class="HAP-listing">
        </ul>
      </aside>
      <section class="HAP--core">
        <div class="group">
          <div class="s-span-1of1 m-span-1of2">
            <div class="HAP-source">
              <div id="source-canvas" class="HAP-canvas HAP-canvas--source"></div>
              <div id="source-transcript" class="HAP-transcript HAP-transcript--source">
                <div class="HAP-notice">
                  <p>{{L.pad__start-creating-remix}}</p>
                </div>
              </div>
            </div>
          </div>
          <div class="s-span-1of1 m-span-1of2">
            <div class="HAP-output">
              <div class="HAP-helpers">
                <a class="HAP-helper" id="HAP-helper"><i class="icon-info"></i></a>
              </div>
              <div id="output-canvas" class="HAP-canvas HAP-canvas--output">
              </div>
              <div id="output-transcript" class="HAP-transcript HAP-transcript--output"></div>
              <div id="effects" class="HAP-toolbar">
                <div class="HAP-toolbar__sec-fraction">
                  <div class="group">
                    <div class="s-span-1of3">
                      <div id="fadeFX" class="HAP-droplet HAP-effect">{{L.pad__fade}}</div>
                    </div>
                    <div class="s-span-1of3">
                      <div id="trimFX" class="HAP-droplet HAP-effect">{{L.pad__trim}}</div>
                    </div>
                    <div class="s-span-1of3">
                      <div id="titleFX" class="HAP-droplet HAP-effect">{{L.pad__title}}</div>
                    </div>
                  </div>
                </div>
                <div class="HAP-toolbar__pri-fraction">
                  <div class="group">
                    <header id="header">
                      <h1 id="mix-title">
                        <form id="mix-title-form">
                          <input type="text" value="Untitled Mix - click to edit" id="mix-title" onfocus="if(this.value=='Untitled Mix - click to edit')this.value='Untitled Mix'" />
                        </form>
                      </h1>
                      <a class="pri-button" id="HAP-share-bttn" data-panel-target="share">{{L.pad__share}}</a>
                      <a class="pri-button" id="share-button"></a>
                      <a class="pri-button" id="save-button" style="display: none !important"></a>
                      <a class="pri-button" id="save-button-saving" style="display: none !important"></a>
                    </header>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>
  </div>
</div>
<!-- **> aside-right** -->
<div id="page-overlay" class="page-overlay toggleSide togglePanel" data-overlay-state="cover-none" data-panel-target="all" data-side-target="all"></div>
<!-- **> panel-language** -->
<div id="panel--language" class="panel" data-panel-state="inactive">
  <div class="panel__head">
    <h2 class="panel__title">{{L.panel-lang__change-lang}}</h2>
  </div>
  <div class="panel__foot">
    <div class="group">
      <div class="s-span-1of1 m-span-3of5 m-center l-span-3of5 l-center">
        <a href="/aje/PalestineRemix/mobile/" class="panel__action--pri togglePanel mbs noenglish" data-panel-target="language">ENGLISH</a>
        <a href="/aja/PalestineRemix/mobile/" class="panel__action--pri togglePanel mbs noarabic" data-panel-target="language">عربي</a>
        <a href="/ajt/PalestineRemix/mobile/" class="panel__action--pri togglePanel mbs noturkish" data-panel-target="language">TÜRKÇE</a>
        <a href="/ajb/PalestineRemix/mobile/" class="panel__action--pri togglePanel nobosnian" data-panel-target="language">BOSANSKI</a>
      </div>
    </div>
  </div>
  <a class="panel__action--sec togglePanel" data-panel-target="language">{{L.panel-lang__cancel}}</a>
</div>
<!-- **> panel-language** -->
<!-- **> panel-share** -->
<div id="panel--share" class="panel" data-panel-state="inactive">
  <div class="panel__head">
    <h2 class="panel__title">{{L.panel-share__share-remix}}</h2>
  </div>
  <div class="panel__body pvh">
    <div class="group">
      <div class="s-span-1of1 m-span-3of5 m-center l-span-3of5 l-center">
        <h3 class="panel__subtitle">{{L.panel-share__share-remix-with}}</h3>
        <div class="group">
          <div class="span-1of4">
            <a id="hap-share-facebook" target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=" class="sec-button block pam mrx"><i class="icon-facebook" style="font-size: 24px;"></i></a>
          </div>
          <div class="span-1of4">
            <a id="hap-share-twitter" target="_blank" href="https://twitter.com/home?status=" class="sec-button block pam mhx"><i class="icon-twitter" style="font-size: 24px;"></i></a>
          </div>
          <div class="span-1of4">
            <a id="hap-share-google" target="_blank" href="https://plus.google.com/share?url=" class="sec-button block pam mhx"><i class="icon-googleplus" style="font-size: 24px;"></i></a>
          </div>
          <div class="span-1of4">
            <a id="hap-share-email" target="_blank" href="mailto:?subject={{L.mailto-subject}}&body=Shared%20from%20Palestine%20Remix" class="sec-button block pam mlx"><i class="icon-mail" style="font-size: 24px;"></i></a>
          </div>
        </div>
        <h3 class="panel__subtitle--alt mtl">{{L.panel-share__save-loc}}</h3>
        <pre id="hap-share-url" class="pre mbl"></pre>
      </div>
    </div>
  </div>
  <div class="panel__foot">
    <div class="group">
      <div class="s-span-1of1 m-span-3of5 m-center l-span-3of5 l-center">
        <a class="panel__action--pri togglePanel" data-panel-target="filter">{{L.panel-share__done}}</a>
      </div>
    </div>
  </div>
  <a class="panel__action--sec togglePanel" data-panel-target="share">{{L.panel-share__close}}</a>
</div>
<!-- **> panel-share** -->
<script src="assets/scripts/vendorq2.js"></script>
<script src="assets/scripts/aj.js"></script>
<script src="assets/scripts/ports.js"></script>
<script type="text/javascript">
AJHAWrapper.init('Pad', '/AJE/PalestineRemix/transcripts/html/' + L + '/', toggleHAPTour);
</script>
</body>
</html>
